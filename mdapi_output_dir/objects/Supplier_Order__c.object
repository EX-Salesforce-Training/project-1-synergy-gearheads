</validationRules>
    <validationRules>
        
    <fullName>Tax_Rate_Rule</fullName>
        
    <active>true</active>
        
    <errorConditionFormula>(Tax_Rate__c  &lt; 0.04 &amp;&amp;  Tax_Rate__c &gt; 0.1)</errorConditionFormula>
        
    <errorDisplayField>Cost__c</errorDisplayField>
        
