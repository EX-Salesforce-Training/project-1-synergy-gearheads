    <validationRules>
        
    <fullName>Card_American_Express_Format</fullName>
        
    <active>true</active>
        
    <errorConditionFormula>ISPICKVAL(Credit_Card_Type__c, &quot;American Express&quot;) &amp;&amp; NOT(REGEX(Credit_Card_Number__c, &quot;[0-9]{4}-[0-9]{6}-[0-9]{4}&quot;))</errorConditionFormula>
        
    <errorDisplayField>Credit_Card_Number__c</errorDisplayField>
        
    <errorMessage>Invalid Format. American Express credit card format is ####-######-####. Please enter a credit card with this formatting.</errorMessage>
        
</validationRules>
    <validationRules>
        
    <fullName>Must_Have_a_CC</fullName>
        
    <active>true</active>
        
    <description>Each Customer must have a complete credit card.</description>
        
    <errorConditionFormula>IF( Credit_Card_Number__c = NULL  || 
 ISPICKVAL(Credit_Card_Type__c, &quot;&quot;) ||
  ISPICKVAL(Credit_Card_Expiration_Month__c, &quot;&quot;) ||
  ISPICKVAL(Credit_Card_Expiration_Year__c, &quot;&quot;), true, false)</errorConditionFormula>
        
    <errorDisplayField>Credit_Card_Type__c</errorDisplayField>
        
    <errorMessage>Each Customer must have complete credit card information to proceed.</errorMessage>
        
</validationRules>
    <validationRules>
        
    <fullName>Visa_Mastercard_Discover_Formatting</fullName>
        
    <active>true</active>
        
    <description>Formats the card number to ####-####-####-#### when it is Visa, Mastercard, or Discover</description>
        
    <errorConditionFormula>NOT(ISPICKVAL(Credit_Card_Type__c, &quot;American Express&quot;)) &amp;&amp; 
NOT(REGEX(Credit_Card_Number__c, &quot;[0-9]{4}-[0-9]{4}-[0-9]{4}-[0-9]{4}&quot;))</errorConditionFormula>
        
    <errorMessage>Incorrect Format. The format for Visa, Mastercard, and Discover is ####-####-####-####. Please enter the card number as such or select a different card.</errorMessage>
        
</validationRules>
